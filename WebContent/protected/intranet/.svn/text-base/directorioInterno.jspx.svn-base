<?xml version="1.0" encoding="utf-8" ?>

<jsp:root version="1.2" 
          xmlns:jsp="http://java.sun.com/JSP/Page"
	      xmlns:f="http://java.sun.com/jsf/core"
	      xmlns:h="http://java.sun.com/jsf/html"
	      xmlns:ice="http://www.icesoft.com/icefaces/component">
	
	<jsp:directive.page contentType="text/html;charset=utf-8"/>
	
	<f:view>
	    
	    <ice:outputDeclaration doctypeRoot="HTML"
				               doctypePublic="-//W3C//DTD HTML 4.01 Transitional//EN"
				               doctypeSystem="http://www.w3.org/TR/html4/loose.dtd" />
	    <html>
		    
		    <head>
		    	<!-- <link rel="stylesheet" href="../../css/style.css" type="text/css" media="screen" /> -->
		    	<!-- <link href="../../xmlhttp/css/xp/xp.css" rel="stylesheet" type="text/css"/> -->
		    	<!-- <link href="../../xmlhttp/css/royale/royale.css" rel="stylesheet" type="text/css"/> -->
		    	<link href="../../xmlhttp/css/rime/rime.css" rel="stylesheet" type="text/css"/>
			</head>
	    	
	        <body>
	        
	        	<ice:form>
					
					<div align="center">
						
						<br />
						
						<table border="0" cellpadding="0" cellspacing="0">
							
							<tr>
								<td style="font-size: 13px; font-weight: bold;" colspan="4" align="center" height="30px;" valign="top">
									<ice:outputText value="#{labelDirectorio.parametros}"/>
								</td>
							</tr>
							
							<tr>
								<td style="font-size: 11px; font-weight: bold; padding-right: 20px; padding-left: 20px;">
									<ice:outputText value="#{labelDirectorio.nombre}"/>
								</td>
								<td style="font-size: 11px; font-weight: bold; padding-right: 20px; padding-left: 20px;">
									<ice:outputText value="#{labelDirectorio.apellido}"/>
								</td>
								<td style="font-size: 11px; font-weight: bold; padding-right: 20px; padding-left: 20px;">
									<ice:outputText value="#{labelDirectorio.dependencia}"/>
								</td>
								<td style="font-size: 11px; font-weight: bold; padding-right: 20px; padding-left: 20px;">
									<ice:outputText value="#{labelDirectorio.empresa}"/>
								</td>
							</tr>
							
							<tr>
								<td style="padding-right: 5px; padding-left: 5px;">
									<ice:inputText style="text-transform:uppercase; width:200px;" maxlength="100"/>
								</td>
								<td style="padding-right: 5px; padding-left: 5px;">
									<ice:inputText style="text-transform:uppercase; width:200px;" maxlength="100"/>
								</td>
								<td style="padding-right: 5px; padding-left: 5px;">
									<ice:inputText style="text-transform:uppercase; width:200px;" maxlength="100"/>
								</td>
								<td style="padding-right: 5px; padding-left: 5px;">
									<ice:inputText style="text-transform:uppercase; width:200px;" maxlength="100"/>
								</td>
							</tr>
							
							<tr>
								<td colspan="4" align="center" valign="bottom" height="30px;">
									<ice:commandButton value="#{labelDirectorio.buscar}" action="#{nominaBean.actionCertificadoLaboral}"/>
								</td>
							</tr>
							
						</table>
						
					</div>
					
				</ice:form>
			
			</body>
		
		</html>		
	
	</f:view>

</jsp:root>